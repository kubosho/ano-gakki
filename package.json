{
  "directories": {
    "test": "./test/scripts/"
  },
  "devDependencies": {
    "browserify": "^6.3.3",
    "espower-loader": "^0.10.0",
    "espowerify": "^0.10.0",
    "intelli-espower-loader": "^0.5.0",
    "karma": "^0.12.23",
    "karma-browserify": "^1.0.0",
    "karma-chrome-launcher": "^0.1.5",
    "karma-mocha": "^0.1.9",
    "mocha": "^1.21.4",
    "power-assert": "^0.10.0"
  },
  "scripts": {
    "global-setup": "npm install -g bower typescript",
    "setup": "npm install && bower install",
    "test": "./node_modules/karma/bin/karma start --single-run",
    "clean": "rm -rf ./build/",
    "compile": "tsc ./src/*.ts --outDir ./build/ --target es5 --module commonjs --removeComments",
    "browserify": "browserify ./build/*.js -d -o ./dist/scripts/ano_gakki.js",
    "build": "npm run clean && npm run compile && npm run test && npm run browserify"
  },
  "private": true
}
